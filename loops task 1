def fibonacci_series(number)
    if(number==0):
       return 0
    else(number==1):
       return 1
    return (fibonacci_series(number - 2)+ fibonacci_series(number - 1))
 n=int(input("enter the value of 'n':"))
 print("fibonacci series:",end = '')
 for n in range(0,n):
  print(fibonacci_series(n),end = '')
